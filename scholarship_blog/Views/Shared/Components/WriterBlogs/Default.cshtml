@model List<Blog>
<div class="tech-btm">
    <h4>Yazarın Diğer Yazıları</h4>
    <h1>@TempData["WriterId"]</h1>

    @foreach (var x in Model)
    {
     
        <div class="blog-grids row mb-3">
            <div class="col-md-5 blog-grid-left">
                <a href="single.html">
                    <img src="@x.BlogThumbnailImage" class="img-fluid" alt="">
                </a>
            </div>
            <div class="col-md-7 blog-grid-right">

                <h5>
                    <a href="/Blog/BlogReadAll/@x.BlogID">@x.BlogTitle </a>
                </h5>
                <div class="sub-meta">
                    <span>
                        <i class="far fa-clock"></i> @x.BlogCreateDate.ToShortDateString()
                    </span>
                </div>
            </div>

        </div>
    }


</div>